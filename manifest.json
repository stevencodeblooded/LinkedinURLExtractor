{
    "manifest_version": 3,
    "name": "LinkedIn URL Extractor",
    "version": "1.1",
    "description": "Extract post URLs from LinkedIn search results",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["*://*.linkedin.com/*"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "background": {
      "service_worker": "background.js"
    }
  }